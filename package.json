{
  "name": "score-beckend",
  "version": "1.0.0",
  "description": "A program that calculates students' scores and is required for a specific training center",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:dev": "nodemon --ext * src/server.js"
  },
  "imports": {
    "#network": "./src/lib/network.js",
    "#config": "./src/config.js",
    "#error": "./src/utils/error.js",
    "#controller/*": "./src/controller/*",
    "#router/*": "./src/routes/*",
    "#middlewares/*": "./src/middlewares/*",
    "#postgres": "./src/utils/postgres.js",
    "#utils/*": "./src/utils/*"
  },
  "keywords": [
    "score",
    "crm", 
    "node.js crm"
  ],
  "author": "Shohijahon Musinkulov",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.3"
  }
}
